{
  "name": "@moose/query-layer",
  "version": "0.0.1",
  "private": true,
  "description": "Type-safe SQL query building helpers for Moose OLAP + ClickHouse. Copy into your project.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "copy:query-layer": "node scripts/copy-query-layer.mjs"
  },
  "dependencies": {
    "@514labs/moose-lib": "^0.6.293",
    "typia": "^10.1.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@confluentinc/kafka-javascript",
      "@514labs/kafka-javascript"
    ]
  },
  "license": "MIT"
}
